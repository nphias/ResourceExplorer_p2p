{
	"name": "resource-explorer",
	"version": "1.0.0",
	"description": "A Visual tool for exploring the resource graph.",
	"repository": "https://github.com/nphias/ResourceExplorer_p2p",
	"homepage": "https://github.com/nphias/ResourceExplorer_p2p#readme",
	"bugs": {
		"url": "https://github.com/nphias/ResourceExplorer_p2p/issues"
	},
	"keywords": [
		"electron",
		"angular",
		"webpack",
		"resource",
		"explorer"
	],
	"author": "Thomas Cal",
	"license": "https://github.com/nphias/ResourceExplorer_p2p#licence",
	"main": "electron.js",
	"scripts": {
		"dev": "tsc electron.ts --outDir . && cross-env NODE_ENV=dev webpack-dev-server --port=4200",
		"prod": "webpack && electron dist/electron.js",
		"start": "electron dist/electron.js",
		"build": "webpack",
		"packager:win": "cross-env NODE_OS=win node builder/packager.js",
		"packager:mac": "cross-env NODE_OS=mac node builder/packager.js",
		"packager:linux": "cross-env NODE_OS=linux node builder/packager.js",
		"installer:win": "npm run packager:win && cross-env NODE_OS=win node builder/installer.js",
		"installer:mac": "npm run packager:mac && cross-env NODE_OS=mac node builder/installer.js",
		"installer:linux": "npm run packager:linux && cross-env NODE_OS=linux node builder/installer.js"
	},
	"devDependencies": {
		"@amcharts/amcharts4": "^4.6.5",
		"@angular/animations": "^8.2.8",
		"@angular/cdk": "^8.2.2",
		"@angular/material": "^8.2.2",
		"canvas": "^2.6.0",
		"clean-webpack-plugin": "^3.0.0",
		"concurrently": "^4.1.1",
		"copy-webpack-plugin": "^5.0.4",
		"cross-env": "^5.2.0",
		"css-loader": "^3.1.0",
		"electron-builder": "^21.2.0",
		"electron-packager": "^14.0.4",
		"extract-loader": "^3.1.0",
		"file-loader": "^4.1.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"imagemin": "^6.0.0",
		"img-loader": "^3.0.1",
		"jsdom": "^16.5.0",
		"lodash": "^4.17.15",
		"node-sass": "^4.12.0",
		"postcss-loader": "^3.0.0",
		"raw-loader": "^3.1.0",
		"resolve-url-loader": "^3.1.0",
		"sass-loader": "^7.1.0",
		"terser-webpack-plugin": "^2.1.2",
		"ts-loader": "^6.0.4",
		"typescript": "^3.5.3",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"underscore-template-loader": "^1.0.0",
		"url-loader": "^2.1.0",
		"webpack": "^4.39.1",
		"webpack-cli": "^3.3.6",
		"webpack-dev-server": "^3.7.2"
	},
	"optionalDependencies": {
		"appdmg": "^0.6.0",
		"electron-installer-debian": "^2.0.0",
		"electron-installer-dmg": "^3.0.0",
		"electron-squirrel-startup": "^1.0.0",
		"electron-winstaller": "^4.0.0"
	},
	"dependencies": {
		"@angular/common": "^8.2.8",
		"@angular/compiler": "^8.2.8",
		"@angular/core": "^8.2.8",
		"@angular/forms": "^8.2.8",
		"@angular/platform-browser": "^8.2.8",
		"@angular/platform-browser-dynamic": "^8.2.8",
		"angular-in-memory-web-api": "^0.9.0",
		"autoprefixer": "^9.6.1",
		"core-js": "^3.1.4",
		"electron": "^6.0.11",
		"rxjs": "^6.5.3",
		"zone.js": "^0.10.2"
	}
}
